%body
  .container
    .jumbotron
      %h1 My Favorite Sites

      -data.sites.each do |site|
        %h1
          %a{href: "#{site.url}"}
            = site.title

      %h4 Fantastic CA curriculum
      - scrape_craft.each do |week|
        %h5= week[:header]
        %p= week[:body]


          Created at:
          = display_date
